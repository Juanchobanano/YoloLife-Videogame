draw_roundrect_colour(
    view_xview[0] + padding_box, 
    padding_box-10,
    view_xview[0] + room_width/2 - padding_box,
    room_height - padding_box + 20,
    c_black,
    c_black,
    0
);
draw_set_alpha(1);
draw_sprite(spr_titulo,3,xx-250, 50);

draw_set_font(font_mensaje);
draw_set_halign(fa_center);
if(idioma == "en"){
    draw_set_color(c_yellow);
    if(!comenzar){
        draw_text(xx,170,"- WEEK NO.2 -");
    }else{
        draw_text(xx,170,"- LEVEL COMPLETE ! -");
    }
    draw_set_color(c_white);
    draw_text(xx, 220, "HIGHSCORE#TABLE");
    if(!instance_exists(obj_hand)){
        instance_create(xx - string_width("HIGHSCORE#TABLE"), 220, obj_hand);
    }
    draw_text(xx, 290, "BEST#SCORE");
    if(file_exists(working_directory + 'tiempo2.sav')){
        draw_text(xx,340,string(Score_tiempo));
    }else{
        draw_text(xx,340,"--.--");
    }
    if(!comenzar){
        draw_sprite(spr_start,0,xx - 95, room_height - padding_box - 40);
    }else{
        draw_sprite(spr_restart,0,xx - 115,room_height - padding_box - 40);
    }
}else if(idioma == "es"){
    draw_set_color(c_yellow);
    if(!comenzar){
        draw_text(xx,170,"- SEMANA NO.2 -");
    }else{
        draw_text(xx,170,"- NIVEL COMPLETO ! -");
    }
    draw_set_color(c_white);
    draw_text(xx, 220, "TABLA DE#PUNTUACION");
    if(!instance_exists(obj_hand)){
        instance_create(xx - string_width("TABLA DE#PUNTUACION"), 220, obj_hand);
    }
    draw_text(xx, 290, "MEJOR#PUNTUACION");
    if(file_exists(working_directory + 'tiempo2.sav')){
        draw_text(xx,340,string(Score_tiempo));
    }else{
        draw_text(xx,340,"--.--");
    }
    if(!comenzar){
        draw_sprite(spr_start,1,xx - 160,room_height - padding_box - 40);
    }else{
        draw_sprite(spr_restart,1,xx - 154,room_height - padding_box - 40);
    }
}

// Start - Comenzar
if(idioma == 'en'){
    if(mouse_check_button(mb_left) and ((mouse_x &gt;= xx - 95 and mouse_y &lt;= xx - 95 + 207) 
    and (mouse_y &gt;= room_height - padding_box - 40 and mouse_y &lt;= room_height - padding_box - 40 + 38))){
        // Start Game or Restart it!
        audio_play_sound(sn_win,1,0);
        if(!comenzar){
            instance_destroy();
        }else{
            with(obj_per) { alarm[1] = 0; } // Detener Polvora.
            if(instance_exists(obj_new)){ with obj_new instance_destroy() }; // Destruir obj new si existe.
            room_restart();
            mouse_clear(mb_left);
        }
    }
}else if(idioma == 'es'){
    if(mouse_check_button(mb_left) and ((mouse_x &gt;= xx - 160 and mouse_y &lt;= xx - 160 + 320) 
    and (mouse_y &gt;= room_height - padding_box - 40 and mouse_y &lt;= room_height - padding_box - 40 + 38))){
        // Comenzar Juego!
        audio_play_sound(sn_win,1,0);
        if(!comenzar){
            instance_destroy();
        }else{
            with(obj_per) { alarm[1] = 0 } // Detener Polvora.
            if(instance_exists(obj_new)){ with obj_new instance_destroy() };
            room_restart();
            mouse_clear(mb_left);
        }
    }
}

// HighScore.
if(idioma == 'en'){
    if(mouse_check_button(mb_left) and (mouse_x &gt;= xx and mouse_x &lt;= xx + string_width("HIGHSCORE#TABLE"))
    and (mouse_y &gt;= 220 and mouse_y &lt;= 220 + string_height("HIGHSCORE#TABLE"))){
        audio_play_sound(sn_win,1,0);
        if (achievement_login_status() == false &amp;&amp; os_is_network_connected()){
            achievement_login();
        }else{
            achievement_show_leaderboards();
            if(Score_tiempo &gt; 0){
                achievement_post_score("CgkIn7v1l5McEAIQDA",floor(Score_tiempo));
                // week 1 : CgkIn7v1l5McEAIQCw
            }
        }
    }
}else if(idioma == 'es'){
    if(mouse_check_button(mb_left) and (mouse_x &gt;= xx and mouse_x &lt;= xx + string_width("TABLA DE#PUNTUACION"))
    and (mouse_y &gt;= 220 and mouse_y &lt;= 220 + string_height("TABLA DE#PUNTUACION"))){
        audio_play_sound(sn_win,1,0);
        if (achievement_login_status() == false &amp;&amp; os_is_network_connected()){
            achievement_login();
        }else{
            achievement_show_leaderboards();
            if (Score_tiempo &gt; 0){
                achievement_post_score("CgkIn7v1l5McEAIQDA",floor(Score_tiempo));
                // week 1 : CgkIn7v1l5McEAIQCw
            }
        }
    }
}
